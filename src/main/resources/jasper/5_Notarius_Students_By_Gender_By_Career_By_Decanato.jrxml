	<queryString language="plsql">
		<![CDATA[SELECT 
		u.id,
		u.nombres, 
		u.cedula,
		u.apellidos,
		case when u.genero = 'M' then 'Masculino' else 'Femenino' end as genero,
		c.nombre as carrera, 
		d.nombre as decanato
FROM usuario u
	JOIN carrera c 
		ON c.id = u.carrera_id
	JOIN decanato d 
		ON d.id = c.decanato_id
WHERE u.estudiante = TRUE
ORDER BY d.nombre
		,c.nombre
		,u.genero
;]]>
	</queryString>
